---
site-title: "inuitcss plugins"
fragment: "#plugins"
---
{{> _page-head}}


<header class="[ page-head ]  [ pv-  mb ]">
    <div class="[ page-wrap ]  [ clearfix ]">
        <h6 class="[ float--left ]  [ mb0 ]"><a class="site-logo" href="index.html{{fragment}}">inuitcss plugins</a></h6>

        <a class="[ site-nav-trigger ]  [ float--right ]" href="#site-nav"></a>
    </div>
</header>


<div class="page-wrap">
    <div class="[ layout  layout--rev ]">
        <div class="[ layout__item  8/12  portable-1/1 ]  [ mb++ ]">
            <h1 class="mb0">{{title}}</h1>
            <div class="mb">
                {{> _source}}<br>
                {{#if demo}}<a class="zeta" href="{{demo}}.html">Demo</a>{{/if}}
            </div>
            {{#markdown}}{{>body}}{{/markdown}}
            {{> _pagination}}
        </div><!--

        --><div class="[ layout__item  4/12  portable-1/1 ]">
            {{> _site-nav plugins}}
        </div>
    </div>
</div>


{{> _page-foot}}


<!-- build:remove -->
<script type="text/javascript">document.write('<script src="http://'
+ (location.host || 'localhost').split(':')[0]
+ ':35729/livereload.js?snipver=1" type="text/javascript"><\/script>')
</script>
<!-- /build -->
</body>
</html>
